# 服务器配置
PORT=8000

# 管理员账号（初始化超级管理员）
ADMIN_USERNAME=superadmin
ADMIN_PASSWORD=your_secure_password_here

# OMDb API 密钥（用于获取电影海报）
# 从 https://www.omdbapi.com/apikey.aspx 获取
OMDB_API_KEY=your_omdb_api_key_here

# CrateDB 数据库配置
CRATEDB_HOST=localhost:5432
CRATEDB_USERNAME=crate
CRATEDB_PASSWORD=your_cratedb_password_here
